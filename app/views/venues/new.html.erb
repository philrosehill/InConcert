<h1>Venues#new</h1>
<p>Find me in app/views/venues/new.html.erb</p>

<%= simple_form_for @venue, html: {class: "form-horizontal"} do |form| %>
	<%= form.input :venue_name %>
	<%= form.input :venue_description %>
	<%= form.input :venue_zip_code %>
	<%= form.submit "Add Venue", class: "btn btn-primary" %>
<% end %>